{
    "url": "kronickle.local",
    "css": {
        "src": "source/scss",
        "dest": "build/-/css",
        "main": "style.scss"
    },
    "js": {
        "src": "source/js",
        "dest": "build/-/js",
        "main": "script.js"
    },
    "img": {
        "src": "source/img",
        "dest": "build/-/img"
    },
    "svg": {
        "src": "source/svg",
        "dest": "build/-/svg"
    },
    "inc": {
        "src": "source/inc",
        "dest": "build/-/inc"
    },
    "php": {
        "src": "source/php",
        "dest": "build"
    },
    "htaccess": {
        "src": "source/htaccess",
        "dest": "build"
    }
}
